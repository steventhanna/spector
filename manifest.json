{
  "manifest_version": 2,

  "name": "Spector",
  "description": "An application to help search through content in a webpage",
  "version": "0.1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
